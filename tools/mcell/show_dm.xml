<tool id="SHOWDM1" name="Show Data Model" force_history_refresh="True" version="0.0.1">
  <description>Displays a JSON Data Model.</description>
  <command interpreter="python">
    <!-- run_mcell.py "${ input1 }" "${ out_file1 }" "." "${ out_file1.id }" -->
    show_dm.py $input $html_out
